<Window x:Class="Demo.Avalonia.CustomDialogTypeLocator.MainWindow"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:Demo.Avalonia.CustomDialogTypeLocator"
    mc:Ignorable="d"
    WindowStartupLocation="CenterScreen"
    Title="Demo - Custom Dialog Type Locator"
    Width="300"
    Height="200"
    d:DataContext="{x:Static local:App.MainWindow}">

    <Grid>
        <Button Command="{Binding ShowDialogCommand}" Content="Open dialog" />
    </Grid>
</Window>
